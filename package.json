{
  "name": "fileshare",
  "version": "1.0.0",
  "description": "App for sharing files.",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w api/src --exec \"babel-node api/src --presets env,stage-0\"",
    "build": "babel api/src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint api/src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "keywords": [],
  "author": "Mounica P",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "lodash": "^4.17.15",
    "mongodb": "^3.4.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "braces": "^3.0.2",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2"
  }
}
